{
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": "google",
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "eslint-plugin-import",
      "eslint-plugin-prefer-arrow"
    ],
    "rules": {
      "max-len": ["error", { "code": 120 }],
      "no-invalid-this": "off",
      "new-cap": ["error", { "newIsCap": true,
       "capIsNewExceptions": ["Given", "When", "Then", "And", "But"] }]
    }
  }
  