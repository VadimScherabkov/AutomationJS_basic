{
    "globals": {
      "$": true,
      "$$": true,
      "browser": true,
      "WebdriverIO": true
    },
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "eslint-plugin-import",
      "eslint-plugin-prefer-arrow"
    ],
    "rules": {
      "max-len": ["error", { "code": 120 }],
      "no-invalid-this": "off"
    }
  }
  